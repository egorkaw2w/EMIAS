<UserControl x:Class="EMIAS.cards.Analize_card"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EMIAS.cards"
             mc:Ignorable="d" 
             d:DesignHeight="64" d:DesignWidth="350">
    <Grid Background="white">
        <Grid Margin="5">
            <Grid.RowDefinitions>
                <RowDefinition Height="1.4*"></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock Text="Определение антигена Coronavirus (SARS-CoV-2) качественно" TextWrapping="Wrap" FontSize="12" TextAlignment="Center"></TextBlock>
            <TextBlock Grid.Row="1" Text="29 декабря 2023 г." TextAlignment="Center" FontSize="10px" VerticalAlignment="Center" Foreground="#807F7F"></TextBlock>
        </Grid>
    </Grid>
</UserControl>
